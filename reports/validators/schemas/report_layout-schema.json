{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "patternProperties": {
        "^.*$": {
            "type": "object",
            "properties": {
                "order": {
                    "type": "integer"
                },
                "width": {
                    "type": "integer"
                }
            },
            "required": [
                "order",
                "width"
            ]
        }
    }
}
